#ejemplo de uso de cola con clase
class Stack:
def __init__ (self):
self.items=[]

def isEmpty (self):
return self.items == []

def push (self, item):
self.items.append(item)

def pop(self):
return self.items.pop()

def top (self):
return self.items[len(self.items)-1]

def size(self):
return len(self.items)

s= Stack() #// creando un objeto de la clase Stack
print (s.isEmpty()) #// True o False
s.push(100) #//metiendo un valor
s.push(200)
s.pop() #// saca un valor
s.size()
